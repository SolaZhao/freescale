<docgen>

<set name="ver_pdf" val="1"/>

<standarddoc/>
<set name="_pdf_contentpages" val="3"/>
<uselib name="templates\beantemplate.xml"/>

<define name="beandoc">
<!-- attrs: name - name of bean -->
<set name="imagepath" val="beans\[[name]]\"/>
<set name="ImageSrcPath" val="<#USER_WORKING_DIRECTORY>beans\[[name]]\"/>
<set name="bean_name" val="[[name]]"/>

<parse text="[[name]]" to="__tmp" splitter="_MPC5500"/>
<chapter name="[[%__tmp]]"
         id="[[name]]"
         file="[[%bean_name]].html"
         usetemplate="beantemplate"
         title="[[%__tmp]]"
         >
<include file="[[name]].xml"/>
</chapter>
</define>


<define name="cpudoc">
<!-- attrs: name - name of bean -->
<set name="imagepath" val="..\CPUs\[[name]]\"/>
<set name="ImageSrcPath" val="..\CPUs\[[name]]\"/>
<set name="bean_name" val="[[name]]"/>

<chapter name="[[name]]"
         id="[[name]]"
         file="[[%bean_name]].html"
         usetemplate="beantemplate"
         title="[[%bean_name]]"
         >
<include file="..\CPUs\[[name]]\[[name]].xml"/>
</chapter>
</define>


<page parser="_pdfparser">

<pdf_newpage/>
<set name="_pdf_font" val="Arial"/>
<set name="_pdf_cury" val="650"/>
<set name="_pdf_font_size" val="50"/>
<pdf_setcurrentfont/>
<pdf_setalign align="center"/>
<br/>
Processor Expert<br/>
<set name="_pdf_cury" val="{{[[%_pdf_cury]] - 20}}"/>

<set name="_pdf_font_size" val="15"/>
<pdf_setcurrentfont/>
  <i>Customized documentation</i><br/>
  <br/>
<set name="_pdf_font_size" val="30"/>
<pdf_setcurrentfont/>

<set name="_pdf_cury" val="400"/>
Appendix A<br/>
Embedded Beans<br/>


<set name="_pdf_font_size" val="10"/>
<pdf_setcurrentfont/>

<set name="_pdf_cury" val="150"/>
version 1.0<br/>
PROCESSOR EXPERT and EMBEDDED BEANS are trademarks of Freescale Semiconductor, Inc.
<br />
Copyright 1997 - 2009 Freescale Semiconductor, Inc.
</page>

<pdf_placecontent/>

<set name="_pdf_toptext_left" val="Processor Expert - Embedded Beans"/>

<set name="pdf_images_jpeg" val="0"/>

<#PDF_BEAN_LIST>

<closedoc filename="<#PDF_TARGET_FILENAME>" title="Bean Documentation"/>


</docgen>

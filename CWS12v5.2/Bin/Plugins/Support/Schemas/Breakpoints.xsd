<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:element name="breakpoints-file">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="breakpoints">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="version" type="xs:integer"/>
							<xs:element name="breakpoint" minOccurs="0" maxOccurs="unbounded">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="id" type="xs:string"/>
										<xs:element name="enabled" type="xs:string"/>
										<xs:element ref="resolved" minOccurs="0"/>
										<xs:element name="position" type="xs:string">
											<xs:annotation>
												<xs:documentation>position = line number for source breakpoints, section id for assembly breakpoints and address for watchpoints.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="offset" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>offset = offset in line for source breakpoints and offset in section for assembly breakpoints. This element is not required for watchpoints.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="length" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>This element is required for watchpoints.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element ref="condition"/>
										<xs:element name="condition-enabled" type="xs:string"/>
										<xs:element name="function-name" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>This element is required for source breakpoints.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="description" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>This element is required for watchpoints.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="property" minOccurs="0" maxOccurs="unbounded">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="name" type="xs:string"/>
													<xs:element name="datatype">
														<xs:complexType>
															<xs:simpleContent>
																<xs:extension base="xs:string">
																	<xs:attribute name="vt" type="xs:string" use="optional"/>
																</xs:extension>
															</xs:simpleContent>
														</xs:complexType>
													</xs:element>
													<xs:element name="value">
														<xs:complexType>
															<xs:simpleContent>
																<xs:extension base="xs:string">
																	<xs:attribute name="other-value" type="xs:string" use="optional"/>
																</xs:extension>
															</xs:simpleContent>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
									<xs:attribute name="type" use="required">
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:enumeration value="source-breakpoint"/>
												<xs:enumeration value="assembly-breakpoint"/>
												<xs:enumeration value="watchpoint"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="resolved" type="xs:string">
		<xs:annotation>
			<xs:documentation>This element is required for source and assembly breakpoints.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="condition" type="xs:string"/>
	<xs:element name="properties">
		<xs:complexType>
			<xs:sequence/>
		</xs:complexType>
	</xs:element>
</xs:schema>

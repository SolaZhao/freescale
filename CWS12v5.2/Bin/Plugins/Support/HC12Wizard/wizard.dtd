<?xml version="1.0" encoding="UTF-8"?>
<?codewarrior wizardversion="1.0" ideversion="5.2" ?>
<!ELEMENT WIZARD (FEATURES?, WIZARDDIALOGCONFIGLIST?, DIALOGLIST?, WIZARDELEMENTLIST?, TEMPLATELIST?, REJECTIONLIST?)>
<!ATTLIST WIZARD
	name CDATA #IMPLIED
>
<!ELEMENT FEATURES (NEWPROJECTWIZARD?, AUTONEWPROJECTLIST?, PROJECTRECONFIGUREWIZARDLIST?, QUICKRECONFIGURELIST?, STARTUPDIALOG?, TOOLBARICONFILES?)>
<!ELEMENT NEWPROJECTWIZARDLIST (NEWPROJECTWIZARD+)>
<!ELEMENT NEWPROJECTWIZARD EMPTY>
<!ATTLIST NEWPROJECTWIZARD
	wizardID IDREF #REQUIRED
>
<!ELEMENT AUTONEWPROJECTLIST (AUTONEWPROJECT+)>
<!ELEMENT AUTONEWPROJECT (FILETYPELIST+)>
<!ATTLIST AUTONEWPROJECT
	wizardID IDREF #REQUIRED
>
<!ELEMENT FILETYPELIST (FILETYPE+)>
<!ELEMENT FILETYPE (WIZARDELEMENTREF*)>
<!ATTLIST FILETYPE
	extension CDATA #REQUIRED
>
<!ELEMENT PROJECTRECONFIGUREWIZARDLIST (PROJECTRECONFIGUREWIZARD+)>
<!ELEMENT PROJECTRECONFIGUREWIZARD (WIZARDELEMENTREF*)>
<!ATTLIST PROJECTRECONFIGUREWIZARD
	menucommand CDATA #REQUIRED
	wizardID IDREF #REQUIRED
	iconindex CDATA #IMPLIED
	backupWizardElementID CDATA #IMPLIED
>
<!ELEMENT QUICKRECONFIGURELIST (QUICKRECONFIGURE+)>
<!ELEMENT QUICKRECONFIGURE (MENUCOMMAND?, TOOLBARITEM?, WIZARDELEMENTREF)>
<!ELEMENT MENUCOMMAND (#PCDATA)>
<!ELEMENT TOOLBARITEM (#PCDATA)>
<!ATTLIST TOOLBARITEM
	kind (ToggleButton | Checkbox | PopupButton | PopupList) #REQUIRED
	iconindex CDATA #IMPLIED
>

<!ELEMENT STARTUPDIALOG (INVOKEWIZARDBUTTON|PROJECTSTATIONERYBUTTON|CHOOSEPROJECTBUTTON|LAUNCHITEMBUTTON)+>
<!ATTLIST STARTUPDIALOG
	menuCommand CDATA #IMPLIED
>
<!ELEMENT INVOKEWIZARDBUTTON EMPTY>
<!ATTLIST INVOKEWIZARDBUTTON
	buttonTitle CDATA #REQUIRED
	wizardID IDREF #REQUIRED
>
<!ELEMENT PROJECTSTATIONERYBUTTON EMPTY>
<!ATTLIST PROJECTSTATIONERYBUTTON
	buttonTitle CDATA #REQUIRED
	dialogTitle CDATA #REQUIRED
	stationeryPath CDATA #REQUIRED
>
<!ELEMENT CHOOSEPROJECTBUTTON EMPTY>
<!ATTLIST CHOOSEPROJECTBUTTON
	buttonTitle CDATA #REQUIRED
>
<!ELEMENT LAUNCHITEMBUTTON (#PCDATA)>
<!ATTLIST LAUNCHITEMBUTTON
	buttonTitle CDATA #REQUIRED
	file CDATA #REQUIRED
>
<!ELEMENT TOOLBARICONFILES EMPTY>
<!ATTLIST TOOLBARICONFILES
	normalIcons CDATA #REQUIRED
	mouseoverIcons CDATA #IMPLIED
>
<!ELEMENT WIZARDDIALOGCONFIGLIST (WIZARDDIALOGCONFIG+)>
<!ELEMENT WIZARDDIALOGCONFIG EMPTY>
<!ATTLIST WIZARDDIALOGCONFIG
	id ID #REQUIRED
	startdialog IDREF #REQUIRED
	title CDATA #REQUIRED
	skipnewdialog (No | Yes) #IMPLIED
	wizardmap (No | Yes) #IMPLIED
	wizardmapBG CDATA #IMPLIED
	allowfinish (No | Yes) #IMPLIED
>

<!ELEMENT DIALOGLIST (DIALOG+)>
<!ATTLIST DIALOGLIST
	idref IDREF #IMPLIED
>
<!ELEMENT DIALOG (TEXTLIST?, IMAGELIST?, INFOAREALIST?, DIALOGITEMLIST?, HELPBUTTONLIST?, NEXTDIALOGSLIST?)>
<!ATTLIST DIALOG
	title CDATA #IMPLIED
	optional (No | Yes) #IMPLIED
	allowHiding (Yes | No) #IMPLIED
	priority CDATA #IMPLIED
	id ID #REQUIRED
>
<!ELEMENT TEXTLIST (TEXT+)>
<!ELEMENT TEXT (CONTENT, RECTANGLE)>
<!ELEMENT CONTENT (#PCDATA)>
<!ELEMENT RECTANGLE EMPTY>
<!ATTLIST RECTANGLE
	left CDATA #REQUIRED
	top CDATA #REQUIRED
	right CDATA #REQUIRED
	bottom CDATA #REQUIRED
>
<!ELEMENT IMAGELIST (IMAGE+)>
<!ELEMENT IMAGE (FILE, RECTANGLE)>
<!ELEMENT FILE ((FIND, REPLACE)*)>
<!ATTLIST FILE
	name CDATA #REQUIRED
	group CDATA #IMPLIED
	priority CDATA #IMPLIED
	linkorder CDATA #IMPLIED
	format (Binary) #IMPLIED
	loopCounter CDATA #IMPLIED
	reconfigAction (Default|Skip|BackupAndReplace|RecreateIfMissing|ReplaceNoBackup) #IMPLIED
	isTemplateFile (No|Yes) #IMPLIED
>
<!ELEMENT FIND (#PCDATA)>
<!ELEMENT REPLACE (#PCDATA)>
<!ELEMENT INFOAREALIST (INFOAREA+)>
<!ELEMENT INFOAREA (USERLIST, POSITION?)>
<!ELEMENT USERLIST (WIZARDELEMENTREF+)>
<!ELEMENT WIZARDELEMENTREF (RECURSION?)>
<!ATTLIST WIZARDELEMENTREF
	idref IDREF #REQUIRED
>
<!ELEMENT RECURSION EMPTY>
<!ATTLIST RECURSION
	nameref CDATA #REQUIRED
	idrefs IDREFS #IMPLIED
>
<!ELEMENT POSITION (FRAME?, RECTANGLE)>
<!ELEMENT FRAME (RECTANGLE)>
<!ELEMENT DIALOGITEMLIST (DIALOGITEM+)>
<!ELEMENT DIALOGITEM (WIZARDELEMENTREF, POSITION?)>
<!ATTLIST DIALOGITEM
	description CDATA #IMPLIED
	presentationmode (Hide | ListSingleSelection | ListMultipleSelection | HierarchicalList | CheckBox | RadioBox | ComboBox | CheckListBox | EditBox | ProjectLocation | AddAdditionalFiles) #REQUIRED
	defaultstate CDATA #IMPLIED
>
<!ELEMENT HELPBUTTONLIST (HELPBUTTON+)>
<!ELEMENT HELPBUTTON (RECTANGLE)>
<!ATTLIST HELPBUTTON title CDATA #IMPLIED>
<!ATTLIST HELPBUTTON helpfile CDATA #REQUIRED>

<!ELEMENT NEXTDIALOGSLIST (NEXTDIALOGS+)>
<!ELEMENT NEXTDIALOGS (DEPENDENCY?, DIALOGREFLIST)>
<!ATTLIST NEXTDIALOGS
	mode (Replace | Add) #IMPLIED
	flow (IgnoreFollowing) #IMPLIED
	skipreconfig (No | Yes) #IMPLIED
>
<!ELEMENT DEPENDENCY (WIZARDELEMENTREF*, DEPENDENCY*)>
<!ATTLIST DEPENDENCY
	kind (AND | OR | NOT | NAND | NOR) #IMPLIED
>
<!ELEMENT DIALOGREFLIST (DIALOGREF+)>
<!ELEMENT DIALOGREF EMPTY>
<!ATTLIST DIALOGREF
	idref IDREF #REQUIRED
>
<!ELEMENT WIZARDELEMENTLIST (WIZARDELEMENT+)>
<!ELEMENT WIZARDELEMENT (DESCRIPTION?, NEXTDIALOGSLIST?, REJECTIONLIST?, RESTRICTIONLIST?, SETTINGSLIST?, EDITBOXSETTINGS?, WIZARDELEMENTLIST?)>
<!ATTLIST WIZARDELEMENT
	name CDATA #REQUIRED
	term CDATA #REQUIRED
	id ID #REQUIRED
	requireSubItems (No | Yes) #IMPLIED
>
<!ELEMENT DESCRIPTION (#PCDATA)>
<!ELEMENT REJECTIONLIST (REJECTION+)>
<!ELEMENT REJECTION (DEPENDENCY?, WIZARDELEMENTREF)>
<!ELEMENT RESTRICTIONLIST (RESTRICTION+)>
<!ELEMENT RESTRICTION (DEPENDENCY?, WIZARDELEMENTREF)>
<!ELEMENT SETTINGSLIST (SETTINGS+)>
<!ELEMENT SETTINGS (TARGET?, DEPENDENCY?, FILELIST?, FOLDERLIST?, SEARCHPATHS?, OPTIONLIST?, SETTINGSLIST?, APPLYTEMPLATE?, MACROLIST?, SUBBUILDTARGETLIST?, LINKEDTARGETLIST?, WIZARDELEMENTLINKS?)>
<!ATTLIST SETTINGS
	flow (IgnoreFollowing) #IMPLIED
>
<!ELEMENT MACROLIST (MACRO+)>
<!ELEMENT MACRO (#PCDATA)>
<!ATTLIST MACRO
	name CDATA #REQUIRED
	kind (Default | Set | Append | GUID | DATE | WIZ_VERSION | PROJECT_NAME_U | PROJECT_NAME_L) #REQUIRED
	sourcefilter CDATA #IMPLIED
	destfilter CDATA #IMPLIED
	replaceInFilenames (Yes | No) #IMPLIED
	replaceInGeneratedFiles (Yes | No) #IMPLIED
	replaceInOptionsValue (Yes | No) #IMPLIED
	mustMatch (Yes | No) #IMPLIED
>
<!ELEMENT TARGET (#PCDATA)>
<!ATTLIST TARGET
	forAll (Yes | No) #IMPLIED
>
<!ELEMENT FILELIST ( COPYFOLDER*, COPYFILE*, REFERENCEFILE*, GENERATEFILE*)>
<!ELEMENT COPYFILE (SOURCE, DESTINATION)>
<!ELEMENT SOURCE (FILE)>
<!ELEMENT DESTINATION (FILE)>
<!ELEMENT REFERENCEFILE (FILE)>
<!ELEMENT GENERATEFILE (FILE, INPUT)>
<!ELEMENT INPUT (#PCDATA)>
<!ELEMENT FOLDERLIST (FOLDER+)>
<!ELEMENT FOLDER (#PCDATA)>
<!ELEMENT COPYFOLDER  (SOURCEFOLDER, DESTINATIONFOLDER)>
<!ELEMENT SOURCEFOLDER (#PCDATA)>
<!ELEMENT DESTINATIONFOLDER (#PCDATA)>
<!ELEMENT SEARCHPATHS (USERSEARCHPATHS?, SYSTEMSEARCHPATHS?)>
<!ELEMENT USERSEARCHPATHS (PATH+)>
<!ELEMENT PATH EMPTY>
<!ATTLIST PATH
	name CDATA #REQUIRED
	reconfigAction (AddIfMissing|RemoveIfUnused) #IMPLIED
	root (Project | Compiler) #IMPLIED
	recursive (Yes | No) #IMPLIED
>
<!ELEMENT SYSTEMSEARCHPATHS (PATH+)>
<!ELEMENT OPTIONLIST ((OPTION | PDATAOPTION)+)>
<!ELEMENT OPTION EMPTY>
<!ATTLIST OPTION
	pluginname CDATA #REQUIRED
	fieldname CDATA #REQUIRED
	value CDATA #REQUIRED
	insertmode (Replace | Add) #IMPLIED
	replaceMacrosInValue (Yes | No) #IMPLIED
>
<!ELEMENT PDATAOPTION EMPTY>
<!ATTLIST PDATAOPTION
	resname CDATA #REQUIRED
	value CDATA #REQUIRED
	reopen (Yes | No) #IMPLIED
>
<!ELEMENT EDITBOXSETTINGS EMPTY>
<!ATTLIST EDITBOXSETTINGS
	macro CDATA #REQUIRED
	regexp CDATA #IMPLIED
	errorMessage CDATA #IMPLIED
>
<!ELEMENT SUBBUILDTARGETLIST (SUBBUILDTARGET+)>
<!ELEMENT SUBBUILDTARGET (REFERENCE_SUBBUILDTARGET)>
<!ELEMENT REFERENCE_SUBBUILDTARGET EMPTY>
<!ATTLIST REFERENCE_SUBBUILDTARGET
	subProjectName CDATA #REQUIRED
	subBuildTargetName CDATA #REQUIRED
	linkIt (Yes | No) #REQUIRED
	buildIt (Yes | No) #REQUIRED
>

<!ELEMENT LINKEDTARGETLIST (LINKEDTARGET+)>
<!ELEMENT LINKEDTARGET EMPTY>
<!ATTLIST LINKEDTARGET
	parentTargetName CDATA #REQUIRED
	linkedTargetName CDATA #REQUIRED
	defaultTargetName CDATA #IMPLIED
>

<!ELEMENT WIZARDELEMENTLINKS (WIZARDELEMENTREF+)>

<!ELEMENT TEMPLATELIST (TEMPLATE)+>
<!ELEMENT TEMPLATE (SETTINGSLIST?)>
<!ATTLIST TEMPLATE
	id ID #REQUIRED
>
<!ELEMENT APPLYTEMPLATE (PARAMETER)*>
<!ATTLIST APPLYTEMPLATE
	idref IDREF #REQUIRED
>
<!ELEMENT PARAMETER (#PCDATA)>
